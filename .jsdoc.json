{
  "source": {
    "include": ["src"],
    "exclude": [
      "src/examples/buffer_by",
      "src/examples/buffer_group_by",
      "src/examples/chunk",
      "src/examples/combine",
      "src/examples/concat",
      "src/examples/delay",
      "src/examples/delay_until",
      "src/examples/filter",
      "src/examples/timeout",
      "src/examples/concat_map"
    ]
  },
  "templates": {
    "default": {
      "outputSourceFiles": true,
      "includeDate": false
    }
  },
  "opts": {
    "template": "node_modules/docdash"
  },
  "plugins": ["plugins/markdown"]
}
